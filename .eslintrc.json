{
  "plugins": [
    "meteor",
    "import"
  ],
  "extends": [
    "standard"
  ],
  "rules": {

    "callback-return": 2,
    "arrow-parens": 2,
    "semi": [
      2,
      "always"
    ],
    "no-multiple-empty-lines": 2,
    "space-before-function-paren": 2,
    "no-var": 2,
    "quotes": [2, "backtick"],
    "prefer-template": 2
  }
}
